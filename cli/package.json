{
  "name": "codex-1up",
  "private": false,
  "type": "module",
  "version": "0.1.0",
  "description": "TypeScript CLI for codex-1up (citty-based)",
  "bin": {
    "codex-1up": "bin/codex-1up.mjs"
  },
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsup src/main.ts --format esm --dts --minify --clean --out-dir dist",
    "test": "vitest",
    "test:run": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "citty": "^0.1.6",
    "execa": "^9.3.0",
    "toml": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.8.5",
    "eslint": "^9.12.0",
    "prettier": "^3.3.3",
    "tsup": "^8.3.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vitest": "^2.1.3"
  },
  "packageManager": "pnpm@10.17.1+sha512.17c560fca4867ae9473a3899ad84a88334914f379be46d455cbf92e5cf4b39d34985d452d2583baf19967fa76cb5c17bc9e245529d0b98745721aa7200ecaf7a",
  "files": [
    "dist",
    "scripts",
    "LICENSE",
    "templates",
    "bin",
    "install.sh",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/regenrek/codex-1up.git"
  },
  "license": "MIT",
  "keywords": [
    "codex",
    "openai",
    "cli",
    "developer-tools"
  ],
  "engines": {
    "node": ">=18"
  }
}
